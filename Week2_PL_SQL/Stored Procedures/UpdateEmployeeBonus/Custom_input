CREATE TABLE employees (
  emp_id     NUMBER PRIMARY KEY,
  emp_name   VARCHAR2(50),
  dept       VARCHAR2(30),
  salary     NUMBER(10,2)
);

INSERT INTO employees VALUES (1, 'John',   'HR',      50000);
